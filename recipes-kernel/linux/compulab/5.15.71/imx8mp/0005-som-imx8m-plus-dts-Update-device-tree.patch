From a44e18e3a1de1ad06dc2420d8136e4d9dd1ea2cf Mon Sep 17 00:00:00 2001
From: Valentin Raevsky <valentin@compulab.co.il>
Date: Fri, 24 Feb 2023 13:42:10 +0200
Subject: [PATCH] som-imx8m-plus: dts: Update device tree

Signed-off-by: Valentin Raevsky <valentin@compulab.co.il>
---
 .../boot/dts/compulab/som-imx8m-plus.dts      | 121 +++++++++++-------
 1 file changed, 75 insertions(+), 46 deletions(-)

diff --git a/arch/arm64/boot/dts/compulab/som-imx8m-plus.dts b/arch/arm64/boot/dts/compulab/som-imx8m-plus.dts
index 20e7be3d7a88..0d3da3af22c8 100644
--- a/arch/arm64/boot/dts/compulab/som-imx8m-plus.dts
+++ b/arch/arm64/boot/dts/compulab/som-imx8m-plus.dts
@@ -12,15 +12,9 @@
 	model = "CompuLab SOM-iMX8M-Plus";
 	compatible = "compulab,som-imx8m-plus", "fsl,imx8mp";
 
-	reserved-memory {
-		#address-cells = <2>;
-		#size-cells = <2>;
-		ranges;
-
-		rpmsg_reserved: rpmsg@0x55800000 {
-			no-map;
-			reg = <0 0x55800000 0 0x800000>;
-		};
+	aliases {
+		ethphy0 = &ethphy0;
+		ethphy1 = &ethphy1;
 	};
 
 	chosen {
@@ -107,32 +101,6 @@
 		regulator-always-on;
 	};
 
-	reg_enet0_reset: regulator-enet0-reset {
-                compatible = "regulator-fixed";
-                regulator-name = "enet1_rst";
-                pinctrl-names = "default";
-                pinctrl-0 = <&pinctrl_enet0_rst>;
-                regulator-min-microvolt = <3300000>;
-                regulator-max-microvolt = <3300000>;
-                gpio = <&gpio1 10 GPIO_ACTIVE_HIGH>;
-                regulator-boot-on;
-                enable-active-high;
-                regulator-always-on;
-        };
-
-	reg_enet1_reset: regulator-enet1-reset {
-                compatible = "regulator-fixed";
-                regulator-name = "enet1_rst";
-                pinctrl-names = "default";
-                pinctrl-0 = <&pinctrl_enet1_rst>;
-                regulator-min-microvolt = <3300000>;
-                regulator-max-microvolt = <3300000>;
-                gpio = <&gpio4 24 GPIO_ACTIVE_HIGH>;
-                regulator-boot-on;
-                enable-active-high;
-                regulator-always-on;
-        };
-
 	sound-hdmi {
 		compatible = "fsl,imx-audio-cdnhdmi";
 		model = "audio-hdmi";
@@ -304,7 +272,10 @@
 	pinctrl-names = "default";
 	pinctrl-0 = <&pinctrl_eqos>;
 	phy-mode = "rgmii-id";
-	phy-handle = <&ethphy0>;
+	phy-handle = <&ethphy1>;
+	snps,force_thresh_dma_mode;
+	snps,mtl-tx-config = <&mtl_tx_setup>;
+	snps,mtl-rx-config = <&mtl_rx_setup>;
 	status = "okay";
 
 	mdio {
@@ -312,10 +283,69 @@
 		#address-cells = <1>;
 		#size-cells = <0>;
 
-		ethphy0: ethernet-phy@0 {
+		ethphy1: ethernet-phy@1 {
 			compatible = "ethernet-phy-ieee802.3-c22";
+			reset-gpios = <&gpio1 10 GPIO_ACTIVE_LOW>;
+			reset-assert-us = <10000>;
+			reset-deassert-us = <80000>;
 			reg = <0>;
 			eee-broken-1000t;
+			realtek,clkout-disable;
+		};
+	};
+
+	mtl_tx_setup: tx-queues-config {
+		snps,tx-queues-to-use = <5>;
+		snps,tx-sched-sp;
+		queue0 {
+			snps,dcb-algorithm;
+			snps,priority = <0x1>;
+		};
+		queue1 {
+			snps,dcb-algorithm;
+			snps,priority = <0x2>;
+		};
+		queue2 {
+			snps,dcb-algorithm;
+			snps,priority = <0x4>;
+		};
+		queue3 {
+			snps,dcb-algorithm;
+			snps,priority = <0x8>;
+		};
+		queue4 {
+			snps,dcb-algorithm;
+			snps,priority = <0xf0>;
+		};
+	};
+
+	mtl_rx_setup: rx-queues-config {
+		snps,rx-queues-to-use = <5>;
+		snps,rx-sched-sp;
+		queue0 {
+			snps,dcb-algorithm;
+			snps,priority = <0x1>;
+			snps,map-to-dma-channel = <0>;
+		};
+		queue1 {
+			snps,dcb-algorithm;
+			snps,priority = <0x2>;
+			snps,map-to-dma-channel = <1>;
+		};
+		queue2 {
+			snps,dcb-algorithm;
+			snps,priority = <0x4>;
+			snps,map-to-dma-channel = <2>;
+		};
+		queue3 {
+			snps,dcb-algorithm;
+			snps,priority = <0x8>;
+			snps,map-to-dma-channel = <3>;
+		};
+		queue4 {
+			snps,dcb-algorithm;
+			snps,priority = <0xf0>;
+			snps,map-to-dma-channel = <4>;
 		};
 	};
 };
@@ -324,7 +354,7 @@
 	pinctrl-names = "default";
 	pinctrl-0 = <&pinctrl_fec>;
 	phy-mode = "rgmii-id";
-	phy-handle = <&ethphy1>;
+	phy-handle = <&ethphy0>;
 	fsl,magic-packet;
 	status = "okay";
 
@@ -332,10 +362,14 @@
 		#address-cells = <1>;
 		#size-cells = <0>;
 
-		ethphy1: ethernet-phy@1 {
+		ethphy0: ethernet-phy@0 {
 			compatible = "ethernet-phy-ieee802.3-c22";
-			reg = <1>;
+			reset-gpios = <&gpio4 24 GPIO_ACTIVE_LOW>;
+			reset-assert-us = <10000>;
+			reset-deassert-us = <80000>;
+			reg = <0>;
 			eee-broken-1000t;
+			realtek,clkout-disable;
 		};
 	};
 };
@@ -875,15 +909,10 @@
 			MX8MP_IOMUXC_SAI1_TXD3__ENET1_RGMII_TD3		0x1f
 			MX8MP_IOMUXC_SAI1_TXD4__ENET1_RGMII_TX_CTL	0x1f
 			MX8MP_IOMUXC_SAI1_TXD5__ENET1_RGMII_TXC		0x1f
+			MX8MP_IOMUXC_SAI2_TXFS__GPIO4_IO24		0x19
 		>;
 	};
 
-        pinctrl_enet1_rst: enet1rst {
-                fsl,pins = <
-			MX8MP_IOMUXC_SAI2_TXFS__GPIO4_IO24		0x19
-                >;
-        };
-
 	pinctrl_flexcan1: flexcan1grp {
 		fsl,pins = <
 			MX8MP_IOMUXC_SAI2_RXC__CAN1_TX          0x154
-- 
2.17.1

