+			eee-broken-1000t;
+		};
+	};
+};
+
+&gpio2 {
+	usb_hub_reset {
+		pinctrl-names = "default";
+		pinctrl-0 = <&pinctrl_usb_hub_rst>;
+		gpio-hog;
+		gpios = <10 GPIO_ACTIVE_HIGH>;
+		line-name = "usb_hub_reset";
+		output-high;
+	};
+};
+
+&gpio4 {
+	usb_vbus_ena {
+		pinctrl-names = "default";
+		pinctrl-0 = <&pinctrl_usb2_vbus>;
